{
  "ignition": {
    "version": "3.2.0"
  },
  "passwd": {
    "users": [
      {
        "name": "core",
        "sshAuthorizedKeys": [
          "ssh-rsa "
        ]
      }
    ]
  },
  "storage": {
    "files": [
      {
        "path": "/etc/ssh/sshd_config.d/20-set-port.conf",
        "contents": {
          "source": "data:,Port%2055522%0A"
        },
        "mode": 420
      },
      {
        "path": "/usr/local/bin/selinux-ssh-port.sh",
        "contents": {
          "compression": "gzip",
          "source": "data:;base64,"
        },
        "mode": 484
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "contents": "[Unit]\nBefore=network-online.target\nConditionPathExists=!/var/lib/selinux-ssh-port\n\n[Service]\nType=oneshot\nExecStart=/usr/local/bin/selinux-ssh-port.sh\nExecStartPost=/usr/bin/touch /var/lib/selinux-ssh-port\nRemainAfterExit=yes\n\n[Install]\nWantedBy=multi-user.target",
        "enabled": true,
        "name": "adjust-selinux-for-ssh-port.service"
      }
    ]
  }
}
